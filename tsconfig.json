{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@api/*": [
        "./api/*"
      ],
      "@api": [
        "./api"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@enums/*": [
        "./shared/enums/*"
      ],
      "@enums": [
        "./shared/enums"
      ],
      "@constants/*": [
        "./shared/constants/*"
      ],
      "@constants": [
        "./shared/constants"
      ],
      "@schemas/*": [
        "./shared/schemas/*"
      ],
      "@schemas": [
        "./shared/schemas"
      ],
      "@store/*": [
        "./shared/store/*"
      ],
      "@store": [
        "./shared/store"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@utils": [
        "./utils"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@hooks": [
        "./hooks"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src",
    ".eslintrc.cjs"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
